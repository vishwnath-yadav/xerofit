<%(0..@num).each do |num|%>
	<div class="exr_field_col del">
	  	<label><%= TargetMuscleGroup::MUSCLES_TYPE[num][0] %></label>
	  	<div class="toggle_rel">
	  	<div class="drop_toggle target_<%=num%>">
	  		<%= (@lib.present?&&@lib.target_muscle_groups.present?) ?  @lib.target_muscle_groups[num].target_muscle_group : "Choose #{TargetMuscleGroup::MUSCLES_TYPE[num][0]}"%>
	  	</div>
	    <div class="fancy_select toggle_abs" style="display:none;">
	    	<%= hidden_field_tag "library[target_muscle_groups_attributes][#{num}][target_muscle_group]", "", class: "target_muscle_menu_#{num}" %>
    		<%= hidden_field_tag "library[target_muscle_groups_attributes][#{num}][sub_target_muscle_group]", "", class: "target_muscle_submenu_#{num}" %>
	    	<ul>
	    		<%TargetMuscleGroup::MUSCLES_TYPE[num][1].each do |key, value|%>
	    			<li class="sub_arw">
	    				<%=key%>
	    				<ul data-group="<%=key%>" data-id="<%=num%>">
	    					<%value.each do |sub_opt|%>
	    						<li data-subgroup="<%=sub_opt%>">
	    							<%=sub_opt%>
	    						</li>
    						<%end%>
	    				</ul>
	    			</li>
	    		<%end%>
	    	</ul>
	    	<%#=select_tag "library[target_muscle_groups_attributes][#{num}][target_muscle_group]", options_for_select(TargetMuscleGroup::MUSCLES_TYPE[num][1]), prompt: "Choose #{TargetMuscleGroup::MUSCLES_TYPE[num][0]}" %>

	    	<%#= hidden_field_tag "library[target_muscle_groups_attributes][#{num}][target_muscle_group]", "", class: "" %>
    		<%#= hidden_field_tag :sub_target_muscle_group, "", class: "" %>
	    </div>
	</div>
	</div>
<%end%>