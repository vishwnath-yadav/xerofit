<ul class="exer_list_view">
  <li>
    <span class="thumb_text">Name</span>
     <span>Type</span>
      <span>Status</span>
       <span>Last Modified</span>
  </li>
  <% @libraries.each_with_index do |library, index| %>
    <li>
      <span class="thumb_text">
        <img src="<%= library.library_video.image %>" class="list_view_img" />
        <%= library.title %>
      </span>
       <span>Single Move</span>
        <span>
          <img src="/assets/green_circle.png" class="list_view_circle" /> 
          <%= library.status %>
        </span>
        <span><%= library.updated_at.strftime("%B %d,%Y") %></span>
    </li>
  <%end%>
</ul>