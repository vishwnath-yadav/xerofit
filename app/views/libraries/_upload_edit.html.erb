<div class="exer_lft_col">
    <h4>Video Details</h4>
    <div class="big_video_col">
       <h5>Video File</h5>
    <%if @library.library_video.present? && @library.library_video.panda_video.present?%>
      <video width="586" height="330" controls>
        <source src="<%= @library.library_video.panda_mp4.url %>" type="video/mp4">
      </video> 
    <%else%>
      Video Not Available
    <%end%>
  </div>

    <div class="thumbnail_col">
      <h5>Video Thumbnail</h5>
      <%= render 'thumbnail' %>
    </div>
    
</div>