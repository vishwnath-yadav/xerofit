<div>
	<%= form_tag('/admin/discover/add_lists', method: :post, id: "add_move_to_list") do |f| %>
		<%= hidden_field_tag "move_id", @move.id %>
		<% if @active_marketplace_list.present? %>
			<div class="move_list">
				<div class="active_list">
					<h4>Active Lists</h4>
					<%= collection_check_boxes(:list_check, :list_ids, @active_marketplace_list, :id, :title, checked: @marketplace_selected_list )%>
				</div>
				<div class="inactive_list">
					<h4>Inactive Lists</h4>
					<%= collection_check_boxes(:list_check, :list_ids, @inactive_marketplace_list, :id, :title, checked: @marketplace_selected_list )%>
				</div>
			</div>
		<% else %>
			<p>There are no Lists to add moves to. Please create a new list.</p>
		<% end %>
	<% end %>
</div>