<table class="table table-bordered table-striped">
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>User Email</th>
        <th>Contant Type</th>
        <th>Date Trashed</th>
        <th>Actions</th>
      </tr>
      <% if moves.present? %>
        <% moves.each do |move| %>
          <tr>
            <td><%=move.id%></td>
            <td><%=move.title %></td>
            <td><%=move.user.email %></td>
            <td><%=move.move_type %></td>
            <td><%= move.user.time_zone.present? ? move.updated_at.in_time_zone(move.user.time_zone).strftime("%m/%d/%y %I:%M%P %Z") : move.updated_at.in_time_zone.strftime("%D %I:%M%P %Z")%></td>
            <td>
              <% if move.move_type == Move::TYPE[1] %>
              <%=link_to "Restore", 'javascript:void(0)', class: "admin_trash_move", "data-params" => "#{move.move_type}_#{move.id}" %> | 
              <%= link_to "Delete", admin_workout_path(move), method: :delete, data: { confirm: 'Are you sure?' }%>
              <% else %>
                <%=link_to "Restore", 'javascript:void(0)', class: "admin_trash_move", "data-params" => "#{move.move_type}_#{move.id}" %> | 
                <%= link_to "Delete", admin_move_path(move), method: :delete, data: { confirm: 'Are you sure?' }%>
              <% end %>
            </td>
          </tr>
        <% end %>
        <% else %>
        <tr><td colspan='6'> No Data Found...</td></tr>
      <% end %>
    </table>

