<h3><%=@user.email%></h3>
<span class="badge badge-success">
    <%= @user.fullname %>
</span>
</br>
<hr>
<%= link_to 'Back', admin_users_path, class: 'btn btn-success' %>
<% if @user.role == "normaluser" %>
	<% if @user.enabled? %>
		<%= link_to 'Disable', disable_admin_user_path(@user), method: :put, class: 'btn btn-success' %>
	<% else %>
		<%= link_to 'Enable', enable_admin_user_path(@user), method: :put, class: 'btn btn-success' %>
	<% end %>
<% end %>