<%if @success == true%>
	$('#image_div').html("<%= escape_javascript(render(:partial => '/common_templates/crop_image_html', locals: {id: @id, type: @type})) %>");
	$(document).ready(function(){
		$("a#testing").trigger("click");
		$.fancybox.close();
	});
<%else%>
  $.fancybox.close();
  alert('Image should be at least 300px by 300px.');
<%end%>
	