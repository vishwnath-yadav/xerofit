<%= form_tag(discover_path, remote: true, method: "get", id: "search_for_discover_home") do |f|%>
    <h4>Browse All</h4>

    <div class="search_bar_col">
        <%= autocomplete_field_tag 'title', '', autocomplete_move_title_discover_index_path, :class => "search_icon search_bar", :placeholder=>"Search", :id => "discover_home_move_title" %>
        <div class="search_bar_icon"></div>
        <div class="search_bar_clear2 hide"></div>
    </div>

    <div class="move-filters">
        <span class="custom_select type_select">
            <%= select_tag :category, options_for_select(@sort_array), prompt: "select category", class: "form-control move_category" %>
        </span>
    </div>
<% end %>