<div class="head hide"></div>

<div class="content hide">
    <div id="circuit-popover" class="block-popover-wrap">
        <input type="hidden" class="sets_count" name="sets" value="1">
        <input type="hidden" class="rest_time" name="rest" value="30">

        <div class="input-section">
            <h5>Sets</h5>
            <div class="number-input-wrap lib_detail_popup">
                <input class="sets_by_popup" max="10" min="1" name="sets_count" step="1" type="number" value="1">
                <label>Total</label>
            </div>
        </div>

        <div class="input-section">
            <h5>Rest</h5>
            <div class="number-input-wrap lib_detail_popup" >
                <input class="sets_by_popup" max="180" min="0" name="rest_time" step="15" type="number" value="30" >
                <label>Seconds</label>
            </div>
        </div>

        <a href="javascript:void(0)" class="remove_block">Delete Circuit</a>
    </div>
</div>