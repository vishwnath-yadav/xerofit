    <div class="workout_block_options">
        <%Block::BLOCK_TYPE.each_with_index do |block, index|%>
            <%if index < 3%>
                <span draggable="true" ondragstart="drag_start(event)" id="dragable_block_<%=index%>" data-dragable-type="Block" data-block-name="<%=Block::BLOCK_TYPE[index]%>">
                    <%=block.humanize%>
                </span>
            <%end%>
        <%end%>
    </div>
