<ul>
  <% @workouts.each_with_index do |workout, index| %>
  	<% if false %>
    <li>
      <a href="#">
        <img src="<%= workout.pic.url() %>" width="231" height="130" alt="cn_1" />
      </a>
      <h6><%= workout.name %></h6>
       <p>
        <img src="<%= status_icon(workout.status)%>" class="list_view_circle" /> 
        <%= workout.status %></p>
    </li>
    <% else %>
      <li>
        <a href="#">
        <img src="/assets/ex_2.jpg" width="231" height="130" alt="cn_1" />
      </a>
      <h6><%= workout.name %></h6>
       <p><img src="<%= status_icon(workout.status)%>" class="list_view_circle" /> <%= workout.status %></p>
    </li>
    <% end %>

  <% end %>
</ul>