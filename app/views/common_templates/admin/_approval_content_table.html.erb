<table class="table table-bordered table-striped">
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Content Type</th>
    <th>Date Submitted for Approval</th>
    <th>Status</th>
    <th>User Email</th> 
    <th>Actions</th>
  </tr>
  <% @moves.each do |move| %>
    <tr>
      <td><%=move.id%></td>
      <td><%=move.title%></td>
      <td><%=move.move_type%></td>
      <td><%=move.date_submitted_for_approval%></td>
      <td>
        <%=render "common_templates/change_status", move: move%>
      </td>
      <td><%=move.user.email %></td>
      <td>
        <%if move.move_type == Move::TYPE[0]%>
          <%=link_to "View", edit_path(move, user: move.user.token)%> | 
        <%else%>
          <%=link_to "View", edit_workout_path(move, user: move.user.token)%> | 
        <%end%>
        <%= link_to "Delete", admin_move_path(move), method: :delete, data: { confirm: 'Are you sure?' }%>
      </td>
    </tr>
  <% end %>
</table>